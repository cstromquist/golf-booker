# JC Golf Tee Time Checker Environment Variables
# Copy this file to .env and fill in your values

# JC Golf Login Credentials (REQUIRED for automatic token refresh)
JC_GOLF_USERNAME=your_email@gmail.com
JC_GOLF_PASSWORD=your_password

# JC Golf API Configuration (auto-populated by token refresh)
JC_GOLF_BEARER_TOKEN=your_bearer_token_here
COURSE_IDS=22,6
CLASS_CODE=JCPWE
MEMBER_STORE_ID=5

# Email Configuration (optional)
EMAIL_USER=your_gmail_address@gmail.com
EMAIL_PASS=your_gmail_app_password
EMAIL_TO=chris.stromquist@gmail.com

# Search Configuration (optional - defaults provided)
# MAX_TEE_TIME=7
# SEARCH_DAYS_AHEAD=14
