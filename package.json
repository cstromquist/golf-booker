{
  "name": "golf-booking-bot",
  "version": "1.0.0",
  "description": "Automated golf tee time booking bot for Lomas Santa Fe Executive Golf Course",
  "main": "scripts/book_golf.js",
  "scripts": {
    "book": "node scripts/book_golf.js",
    "check-tee-times": "node src/scripts/jc_golf_tee_time_checker.js",
    "refresh-token": "node src/scripts/get_jc_golf_token.js",
    "view-summary": "./scripts/view_latest_summary.sh",
    "setup": "./scripts/setup.sh",
    "setup-env": "./scripts/setup_env.sh",
    "setup-tee-checker": "./scripts/setup_tee_time_checker.sh",
    "deploy": "./scripts/deploy.sh",
    "build": "docker build -t golf-booking-bot .",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "docker-logs": "docker-compose logs -f"
  },
  "keywords": [
    "golf",
    "booking",
    "automation",
    "playwright",
    "bot"
  ],
  "author": "Golf Booking Bot Team",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "crypto": "^1.0.1",
    "moment": "^2.29.4",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
