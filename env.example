# Golf Booking Bot Configuration
# Copy this file to .env and fill in your actual values

# Golf Course Website
GOLF_COURSE_URL=https://lomas-santa-fe-executive-golf-course.book.teeitup.com/teetimes

# User Credentials (REQUIRED for Smart Booking)
GOLF_EMAIL=your_email@example.com
GOLF_PASSWORD=your_password_here

# Payment Information (REQUIRED for Smart Booking)
CREDIT_CARD=1234 5678 9012 3456
CVV=123
BILLING_ADDRESS=123 Main Street
POSTAL_CODE=12345

# Legacy Bot Configuration (for original bot)
GOLF_USERNAME=your_username_here
TARGET_DATE=2024-01-15
PREFERRED_TIMES=06:00,06:30,07:00,07:30,08:00
PLAYER_COUNT=2
PLAYER_NAMES=John Doe,Jane Smith

# Notification Settings
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/DISCORD/WEBHOOK
EMAIL_NOTIFICATIONS=true
EMAIL_ADDRESS=your_email@example.com

# Bot Configuration
HEADLESS_MODE=true
RETRY_ATTEMPTS=3
RETRY_DELAY=2000
BOOKING_TIMEOUT=30000
SCREENSHOT_ON_ERROR=true

# Security
ENCRYPTION_KEY=your_32_character_encryption_key_here
SESSION_TIMEOUT=300000

# Logging
LOG_LEVEL=info
LOG_FILE=logs/golf-bot.log
MAX_LOG_SIZE=10MB
MAX_LOG_FILES=5
